{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "expect": true
  },
  "plugins": [ "@typescript-eslint" ],
  "extends": "eslint-config-egg",
  "rules":{
    "linebreak-style": "off",
    "no-unused-vars": 0,
    "strict": 0,
    "no-extend-native": 0
  }
}
